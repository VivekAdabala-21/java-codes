class Solution {
    public boolean isValid(String s) 
    {
       Stack<Character> list=new Stack<>();
       for(int i=0;i<s.length();i++)
       {
        char ch=s.charAt(i);
       
       if(ch=='('||ch=='{'||ch=='[')
       {
        list.push(ch);
       }
       else
       {
        if(list.isEmpty())
        {
            return false;
        }
        char top=list.pop();
        if(ch==')'&& top!='('||ch=='}' && top!='{'||ch==']' && top!='[')
        {
            return false;
        }
       }
       }
       return list.isEmpty();
    }
}
