class Solution {
    public int arrayPairSum(int[] nums) 
    {
        Arrays.sort(nums);
        int n=nums.length;
        int sum=0;
        for(int i=0,j=1;i<n-1 && j<n;i=i+2,j=j+2)
        {
           int min=Math.min(nums[i],nums[j]);
           sum=sum+min;
        }
        return sum;
    }
}
