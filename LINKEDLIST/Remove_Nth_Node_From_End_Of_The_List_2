 ListNode temp1 = head;
        int count = 1;
        while (temp1.next != null) 
        {
            count++;
            temp1 = temp1.next;
        }
        if (n == count) 
        {
            return head.next;
        }
        int data = count - n; 
        int i = 1;
        temp1 = head;

        while (temp1 != null && temp1.next != null && i < data) 
        {
            temp1 = temp1.next;
            i++;
        }

        temp1.next = temp1.next.next;
        return head;
