public class Animal 
{
   void sound()
   {
	   System.out.println("animal makes a sound");
   }
   
}
class Dog extends Animal
{
	@Override
	void sound()
	{
		System.out.println("Dog barks");
	}
}
//MAIN METHOD
public class Animal_Test
{
  public static void main(String[]args)
  {
	  Animal a =new Dog();
	 
	  a.sound();// print subclass
	 
  }
}
