//FILE-1

public class Engine 
{
  private int engModel;
  private String engVariant;
  public Engine(int engModel, String engVariant) {
	super();
	this.engModel = engModel;
	this.engVariant = engVariant;
  }
  @Override
  public String toString() {
	return "Engine [engModel=" + engModel + ", engVariant=" + engVariant + "]";
  }
  
}

//FILE-2

public class Car 
{
  private int carNumber;
  private String carColor;
  private String carCompany;
  private Engine engine;
  public Car(int carNumber, String carColor, String carCompany) {
	super();
	this.carNumber = carNumber;
	this.carColor = carColor;
	this.carCompany = carCompany;
	this.engine = new Engine(15,"BS-13");
  }
  @Override
  public String toString() {
	return "Car [carNumber=" + carNumber + ", carColor=" + carColor + ", carCompany=" + carCompany + ", engine="
			+ engine + "]";
  }  
}


//FILE-3

public class Composition 
{
	public static void main(String[]args)
	{
		Car c= new Car(2221,"white","AUDI");
		System.out.println(c);
	}

}
