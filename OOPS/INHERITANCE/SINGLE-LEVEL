package practice;

public class Inheritance 
{
   public void rohit()
   {
	   String name="rohit";
	   int runs=20000;
	   int centuries=33;
	   int doubleCenturies=3;
	   String role="INDIAN CAPTAIN";
	   
	   System.out.println("name:"+name+"  runs:"+runs+"  centuries"+centuries+"  doubleCenturies"+doubleCenturies+"  role"+role);
	   
   }

   
   
}
class Sub_Inhertance extends Inheritance
{
	public void virat()
	   {
		   String name="virat";
		   int runs=28000;
		   int centuries=53;
		   int doubleCenturies=1;
		   String role="INDIAN KING";
		   
		   System.out.println("name:"+name+"  runs:"+runs+"  centuries"+centuries+"  doubleCenturies"+doubleCenturies+" role"+role);
		   
	   }
}


/MAIN METHOD
package practice;

public class Inheritance_Test 
{
  public static void main(String[]args)
  {
	  Sub_Inhertance a = new Sub_Inhertance();
	  a.virat();
	  a.rohit();
  }
} 

