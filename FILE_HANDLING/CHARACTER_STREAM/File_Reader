package Files;
import java.io.FileReader;
public class File14 {
	public static void main(String[]args) throws Exception
	{
		
		FileReader fw= new FileReader("D:\\vivek\\8.txt");
		
		try(fw)
		{
			int i;
			while((i=fw.read())!=-1)
			{
				System.out.print((char)i);
			}
			
		}
		catch(Exception e)
		{
			System.out.print(e);
		}
	}

}
