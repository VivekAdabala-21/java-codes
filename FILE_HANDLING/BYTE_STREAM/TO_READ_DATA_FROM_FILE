package Files;
import java.io.FileInputStream;
import java.io.IOException;


public class File5
{
	public static void main(String[]args) throws Exception
	{
	  	try(var fis=new FileInputStream("D:\\vivek\\1.txt"))
	  	{
	  		int i;
	  		while((i=fis.read())!=-1)
	  		{
	  			System.out.print((char)i);
	  		}
	  		
	  	}
	  	catch(Exception e)
	  	{
	  		System.out.print(e);
	  	}
	  	System.out.println();
	}
}
